{
  "manifest_version": 3,
  "name": "as.mp4",
  "version": "0",
  "description": "Treat .mov and .m4v files as .mp4",
  "icons": {
    "128": "128.png"
  },
  "omnibox": {
    "keyword": "as.mp4"
  },
  "permissions": [
    "downloads",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "bg.js"
  },
  "action": {}
}